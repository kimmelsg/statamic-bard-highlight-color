{
  "require": {
    "php": "^7.4 | ^8.0 | ^8.1 | ^8.2",
    "statamic/cms": "^3.4"
  },
  "name": "kimmelsg/statamic-bard-highlight-color",
  "keywords": ["statamic", "bard", "highlight-color"],
  "description": "Statamic Bard Highlight Color",
  "autoload": {
    "psr-4": {
      "Kimmelsg\\BardHighlightColor\\": "src"
    }
  },
  "type": "statamic-addon",
  "extra": {
    "statamic": {
      "name": "Statamic Bard Highlight Color",
      "description": "Highlight Color plugin for statamic bard"
    },
    "laravel": {
      "providers": ["Kimmelsg\\BardHighlightColor\\ServiceProvider"]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
